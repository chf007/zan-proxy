{
  "name": "zan-proxy",
  "version": "4.0.27",
  "description": "前端代理",
  "scripts": {
    "dev": "ts-node -P tsconfig.json src/bin/index.ts",
    "dev:site": "cd ./webui && npm run serve",
    "build": "tsc && cd ./webui && npm run build",
    "test":
      "HOME=/tmp/proxy-test nyc mocha -r ts-node/register -c --recursive 'test/**/*.spec.ts'",
    "postinstall": "node ./scripts/resetDataFile.js"
