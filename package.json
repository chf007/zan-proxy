    "ip": "^0.3.2",
    "jsonfile": "^2.4.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^2.3.0",
    "koa-compose": "^4.0.0",
    "koa-favicon": "^2.0.1",
    "koa-mount": "^2.0.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.2.1",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.11",
    "lru-cache": "^4.0.2",
    "mime-types": "^2.1.22",
    "node-fetch": "^2.3.0",
    "node-localstorage": "^1.3.1",
    "npm": "^5.7.1",
    "open": "0.0.5",
    "ora": "^2.0.0",
    "parse-domain": "^1.1.0",
    "pem": "^1.14.2",
    "raw-body": "^2.3.3",
    "reflect-metadata": "^0.1.13",
